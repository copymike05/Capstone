			</div>
		</footer><!-- .site-footer -->
	</div><!-- .page-box -->

	
	@Scripts.Render("~/Scripts/jquery-migrate-1.4.1.min.js")
	@Scripts.Render("~/Scripts/tether.min.js")
	@Scripts.Render("~/Scripts/appear.min.js")
	@Scripts.Render("~/Scripts/bootstrap.min.js")
	@Scripts.Render("~/Scripts/owl.carousel.min.js")
	@Scripts.Render("~/Scripts/jquery.touchwipe.min.js")
	@Scripts.Render("~/Scripts/jquery.magnific-popup.min.js")
	@Scripts.Render("~/Scripts/masonry.pkgd.min.js")
	@Scripts.Render("~/Scripts/main.js")
	@RenderSection("Scripts",false)
</body>
</html>