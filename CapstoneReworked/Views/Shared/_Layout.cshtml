
<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<title>@ViewBag.Title Capstone Project</title>
	<meta name="keywords" content="HTML5 Template">
	<meta name="description" content="Xblog — Multipurpose Blogging Template">
	<meta name="author" content="Next Item">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="format-detection" content="telephone=no">

	<!-- Favicon -->
	<link rel="shortcut icon" href="img/favicon.ico">

	<!-- Plagins CSS -->
	<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
	<link href="~/Content/font-awesome.min.css" rel="stylesheet" />
	<link href="~/Content/owl.carousel.min.css" rel="stylesheet" />
	<link href="~/Content/animate.min.css" rel="stylesheet" />
	<link href="~/Content/magnific-popup.css" rel="stylesheet" />


	<!-- Theme CSS -->
	<link href="~/Content/style.css" rel="stylesheet" />
	@Scripts.Render("~/Scripts/jquery-3.1.1.min.js")
</head>
<body class="fixed-header">
	<div class="preloader"><div class="loader"></div></div>

	<div class="page-box">
		<header class="site-header">
			<div class="container">
				<div class="row">
					<div class="col col-xs-6 col-md-3">
					</div>

					<div class="col col-xs-6 col-md-9 right-align">
						<div class="menu main-menu">
							<a href="#" class="menu-btn"><span></span><span></span><span></span></a>

							<nav class="menu-list-wrap">
								<ul class="menu-list">
									<li class="menu-item menu-item-has-children">@Html.ActionLink("HOME", "Index", "Home")</li>
									<li class="menu-item menu-item-has-children">@Html.ActionLink("RSVP", "Create", "RSVPs")</li>
									<li class="menu-item menu-item-has-children">@Html.ActionLink("EVENT TIMELINE", "Event", "Home")</li>
									<li class="menu-item menu-item-has-children">@Html.ActionLink("CONTACT US", "Contact", "Home")</li>


								</ul>
							</nav><!-- .menu-list-wrap -->
						</div><!-- .main-menu -->
					</div>
				</div>
			</div>
		</header><!-- .site-header -->


		@RenderBody()
		<!-- Footer-->
		<footer class="site-footer">

			<div class="footer-banners">


				<ul>
					<li>
						<a href="#">
							<img class="retina" src="~/Images/IMG_0632.jpg" width="240" height="240" alt="">
